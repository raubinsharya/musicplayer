<fx:root maxHeight="315.0" maxWidth="264.0" prefHeight="315.0" prefWidth="264.0" spacing="5.0" styleClass="vbox" stylesheets="@../css/album.css" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imageView" fitHeight="226.0" fitWidth="229.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/spotify.png" />
         </image>
         <VBox.margin>
            <Insets left="30.0" top="10.0" />
         </VBox.margin>
      </ImageView>
      <Text fx:id="albumName" fill="#f8f7f7" strokeType="OUTSIDE" strokeWidth="0.0" text="Baahubali Original Motion" textAlignment="CENTER" wrappingWidth="267.0">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Text>
      <Text fx:id="artistName" fill="#cdc8c8" strokeType="OUTSIDE" strokeWidth="0.0" text="MM. Kervani" textAlignment="CENTER" wrappingWidth="290.0">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
   </children>
</fx:root>
